{
  "expo": {
    "name": "TaskLS",
    "slug": "taskls",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "taskls",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#042131"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.priyankg85.taskls"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#042131"
      },
      "package": "com.priyankg85.taskls"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3b412207-fc3f-4d30-9d44-6722cd079df1"
      }
    },
    "owner": "priyankg85"
  }
}
